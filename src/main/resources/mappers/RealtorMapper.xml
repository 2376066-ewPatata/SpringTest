<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.irang.springtest.mvc.repository.RealtorRepository">
    <insert id="insertRealtorByObject" parameterType="com.irang.springtest.mvc.domain.Realtor"
            useGeneratedKeys="true" keyProperty="id">
        INSERT INTO
            `realtor`
        (
            `id`
            , `office`
            , `phone_number`
            , `address`
            , `grade`
        )
        VALUE
        (
            #{id}
            , #{office}
            , #{phoneNumber}
            , #{address}
            , #{grade}
        )

    </insert>

</mapper>